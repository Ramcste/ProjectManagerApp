@model System.Web.Mvc.HandleErrorInfo

@{ 
    Layout = "~/Views/Shared/_LoginLayout.cshtml";
}

@{
    ViewBag.Title = "Error";
}

@{
    if (String.IsNullOrEmpty(ViewBag.errorMessage))
    {
        <center><h2 class="text-danger" style="margin-top:30px;">error occurred while processing your request.</h2></center>
    }
    else
    {
        <center><h2 class="text-danger" style="margin-top:30px;">@ViewBag.errorMessage</h2></center>
    }
}


