@model IEnumerable<ProjectManagerApp.Areas.Admin.Models.AspNetUser>
@using ProjectManagerApp.Areas.Admin.DAL

@if (Model.Count() > 0)
{

    <div class="table-responsive">
        <table class="table table-striped table-hover table-bordered">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Email)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.UserName)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Address)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.PhoneNumber2)
                </th>

                <th>
                    @Html.DisplayNameFor(model => model.PhoneNumber)
                </th>


                <th></th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Email)
                    </td>

                    <td>
                        @Html.DisplayFor(modelItem => item.UserName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Address)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.PhoneNumber2)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.PhoneNumber)
                    </td>


                    <td>
                        @Html.ActionLink(" ", "Edit", new { controller = "AspNetUsers", id = item.Id }, new { @class = "fa fa-edit", title = "Edit", id = "btnedit" }) |
                        @Html.ActionLink(" ", "Details", new { controller = "AspNetUsers", id = item.Id }, new { @class = "fa fa-list", title = "Details", id = "btndetails" }) |
                        @Html.ActionLink(" ", "Delete", new { controller = "AspNetUsers", id = item.Id }, new { @class = "fa fa-trash-o", title = "Delete", id = "btndelete" })

                    

                    </td>

                </tr>
            }

        </table>
    </div>

  

}

else
{
    <div class="info">There is no data </div>
}



<script>

    $(function () {
        $('#btnedit').click(function () {
            $('#divLoader').show();
            setTimeout(function () {

            }, 1);

        });
        $('#btndetails').click(function () {
            $('#divLoader').show();
            setTimeout(function () {

            }, 1);

        });
        $('#btndelete').click(function () {
            $('#divLoader').show();
            setTimeout(function () {

            }, 1);

        });
    });

</script>